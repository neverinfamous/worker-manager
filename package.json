{
    "name": "worker-manager",
    "version": "0.1.0",
    "private": true,
    "description": "Cloudflare Pages & Workers management platform",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit",
        "check": "npm run lint && npm run typecheck",
        "deploy": "npm run build && npx wrangler deploy",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage"
    },
    "keywords": [
        "cloudflare",
        "workers",
        "pages",
        "management",
        "typescript"
    ],
    "author": "Adamic.tech",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/neverinfamous/worker-manager.git"
    },
    "bugs": {
        "url": "https://github.com/neverinfamous/worker-manager/issues"
    },
    "homepage": "https://github.com/neverinfamous/worker-manager#readme",
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@radix-ui/react-accordion": "^1.2.11",
